import java.util.Map;
import java.util.HashMap;
import java.util.Stack;

public class NextGreaterElementI {
    public int[] nextGreaterElement(int[] findNums, int[] nums) {
    	Map<Integer, Integer> map = new HashMap<>();
		Stack<Integer> stack = new Stack<>();
		for (int i : nums) {
			System.out.printf("processing %d %n", i);
			while (!stack.empty() && stack.peek()<i ) {
				System.out.printf("pop %d and put into map: %d - %d %n", stack.peek(), stack.peek(), i);
				map.put(stack.pop(), i);
			}
			stack.push(i);
			System.out.printf("%d enter stack %n", i);
			System.out.println(stack);
		}
		for (int i = 0; i < findNums.length; i++) {
			findNums[i] = map.getOrDefault(findNums[i], -1);
		}
		return findNums;
    }

    public static void main(String[] arg) {
    	NextGreaterElementI tester = new NextGreaterElementI();
    	// int[] nums1 = {4,1,2};
    	// int[] nums2 = {1,3,4,2};
		int[] nums1 = {4,1,2};
		int[] nums2 = {1,2,3,4};
    	for (int i : tester.nextGreaterElement(nums1, nums2)) System.out.println(i);
    }
}